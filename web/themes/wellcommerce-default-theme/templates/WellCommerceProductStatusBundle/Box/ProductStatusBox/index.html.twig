{% if dataset.pagination.totalRows > 0 %}
	{% if app.request.attributes.get('_route') == 'front.home_page.index' %}
	{% include 'WellCommerceProductStatusBundle:Front/ProductStatus:products.html.twig' %}
	{% else %}
		{% if app.request.get('view') == 'list' %}
			{% include 'WellCommerceAppBundle:Front/Common:products_list.html.twig' %}
		{% else %}
			{% include 'WellCommerceAppBundle:Front/Common:products.html.twig' %}
		{% endif %}
	{% endif %}

{% else %}
    <div class="alert alert-info" role="alert">
        <strong>{% trans %}category.flash.empty_category{% endtrans %}</strong>
    </div>
{% endif %}
