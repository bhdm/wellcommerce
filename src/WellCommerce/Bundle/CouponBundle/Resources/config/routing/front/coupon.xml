<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="front.coupon.add" path="/add">
        <default key="_controller">coupon.controller.front:addAction</default>
        <option key="expose">true</option>
    </route>

    <route id="front.coupon.delete" path="/delete">
        <default key="_controller">coupon.controller.front:deleteAction</default>
        <option key="expose">true</option>
    </route>
</routes>
