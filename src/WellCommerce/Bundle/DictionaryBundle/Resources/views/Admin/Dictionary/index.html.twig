{% extends 'WellCommerceAdminBundle:Admin:layout.html.twig' %}
{% trans_default_domain "wellcommerce" %}
{% block content %}
    <h2>{% trans %}dictionary.heading.index{% endtrans %}</h2>
    <ul class="possibilities">
        <li><a href="{{ path('admin.dictionary.sync') }}" class="button"><span>{% trans %}dictionary.button.sync{% endtrans %}</span></a></li>
        <li><a href="{{ path('admin.dictionary.add') }}" class="button" title="{% trans %}dictionary.button.add{% endtrans %}"><span>{% trans %}dictionary.button.add{% endtrans %}</span></a></li>
    </ul>
    {{ datagrid_renderer(datagrid) }}
{% endblock %}
